<queue-entries>
<!--Change the refresh rate of the default queue to 1/s-->
  <queue>
    <name>default</name>
    <rate>10/m</rate>
    <max-concurrent-requests>1</max-concurrent-requests>
    <retry-parameters>
      <task-retry-limit>1</task-retry-limit>
    </retry-parameters>
  </queue>
</queue-entries>